이 아래로는 테스트/추가 예정

  "j": {
    "뜻": "-들",
    "설명": "복수형",
    "예문": "aphamuj : 오리들"
  },
  "jen": {
    "뜻": "-보다",
    "설명": "비교",
    "예문": "(b) epa dik jen (a) : a보다 b가 크다"
  },
  "'n": {
    "뜻": "-이 아닌",
    "설명": "부정",
    "예문": "ti epa'n aphamu : 그것은 오리가 아니다. i pamunu'n ti : 나는 그것을 튀기지 않았다." 
  }

  
  "'vi": {
    "뜻": "되다",
    "설명": "피동. pamunu'vi = 튀겨지다"
  },
  "yi": {
    "뜻": "할 것이다",
    "예문": "yi pamunud -> 튀길 것이고",
    "변형":{
      "yiv": "될 것이다"
    }
  },
  "yui": {
    "뜻": "했다",
    "예문": "yui pamunud -> 튀겼고",
    "변형":{
      "yuv": "됐다"
    }
  },
  "'pi": {
    "뜻": "하고 있다",
    "설명": "진행형. pamunu'pi = 튀기고 있다.",
    "변형": {
      "'piv": "되고 있다",
      "'piy": "하고 있었다",
      "'pivy": "되고 있었다"
    }
  },
  "'si": {
    "뜻": "했을 것이다",
    "설명": "추정하는 문장에 쓰임. su pamunu'si ti = 그녀는 그것을 튀겼을 것이다.",
    "변형":{
      "'siv": "됐을 것이다."
    }
  } 


  "sus": {
    "뜻": "또한",
    "예문": "su epat en, i epa en sus. -> 그녀가 사람이라면, 나 또한 사람이다. / hu epa en. i epa en sus -> 그는 사람이다. 나 또한 사람이다."
  },
  "sos": {
    "뜻": "때문에",
    "예문": "ti sos -> 그것 때문에. ti pamunu'v sos pamun -> 그것은 기름 때문에 튀겨졌다."
  },
  "es": {
    "뜻": "또는",
    "예문": "ti epa kamu es omu -> 그것은 개 또는 고양이이다."
  }

  "-eiy": {
    "뜻": "-주의",
    "설명": "사상이나 체제를 나타냄. emmin(사회) + -ey(-주의) -> emmineiy(사회주의)"
  },
  "-aiy": {
    "뜻": "-적인 / -같은",
    "설명": "en(사람) + -aiy(-적인/-같은) -> enaiy(인간적인/사람같은)"
  }


  "teoj": {
    "뜻": "그",
    "설명": "뜻은 the와 같지만, teoj가 붙은 것은 이미 언급한 내용. 붙지 않은 것은 새 정보이다. 'i loku mu. teoj mu epa kamu. -> 나는 동물(새 정보)을 본다. (방금 말한) 그 동물은 개이다.'"
  },
  "toi": {
    "뜻": "이것",
    "예문": "tio epa mu -> 이것은 동물이다"
  },
  "tai": {
    "뜻": "저것",
    "예문": "tao epa mu -> 저것은 동물이다"
  }

